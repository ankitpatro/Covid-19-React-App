{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": false
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": [0],
    "import/extensions": "off",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-use-before-define": "off",
    "react/prop-types": "off",
    "react/jsx-wrap-multilines": "off"
  }
}
